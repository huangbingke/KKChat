  use_frameworks!
  pod 'Masonry'
  pod 'SVGKit', :git => 'https://github.com/SVGKit/SVGKit.git', :branch => '3.x'
  pod 'AFNetworking'
  pod 'SDWebImage'
  pod 'FMDB'
  pod 'YYText'
  pod 'SocketRocket'
end
